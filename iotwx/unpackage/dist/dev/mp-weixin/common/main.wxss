@font-face {
  font-family: "iconfont"; /* Project id 3441281 */
  src: url(data:font/ttf;base64,) format('truetype');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
