/* 全局 */
page {
  --mainColor: $mainColor;
}
/* 布局 */
.flex-center {
  display: flex;
  justify-content: center;
  align-items: center;
}
.mgt-20 {
  margin-top: 20rpx;
}
.mgt-50 {
  margin-top: 50rpx;
}
/* 表单样式 */
.text-plh {
  color: #d2d2d2;
}
.data {
  padding: 0 50rpx;
  font-family: "微软雅黑", "Micosoft Yahei", "Micosoft YaHei", "sans-serif";
  color: #464646;
}
.curDev {
  padding-top: 30rpx;
  width: 100%;
  height: 100rpx;
  border-bottom: 1px solid #e6e6e6;
  display: flex;
  align-items: center;
  position: relative;
}
.curDev text {
  color: #464646;
  font-weight: bold;
  font-family: "微软雅黑", "Micosoft Yahei", "Micosoft YaHei", "sans-serif";
}
.curDev button {
  right: 0;
  position: absolute;
  float: right;
}
.subMsg {
  width: 100%;
  height: 200rpx;
  padding: 20rpx;
  box-shadow: 1px 1px 1px 1px gainsboro;
  border-radius: 5rpx;
  box-sizing: border-box;
}
.subMsg switch {
  margin-right: 20rpx;
}
.subMsg .msg-box {
  padding-left: 15rpx;
  font: 35rpx/70rpx "微软雅黑", "Micosoft Yahei", "Micosoft YaHei", "sans-serif";
  width: 100%;
  height: 70rpx;
  border: 1px solid #c8c8c8;
  border-radius: 5px;
  box-sizing: border-box;
}
.subD {
  width: 100%;
}
.subD:last-of-type {
  margin-bottom: 150rpx;
}
.subD .subData {
  width: 100%;
  height: 510rpx;
  padding: 20rpx;
  box-shadow: 1px 1px 1px 1px gainsboro;
  border-radius: 5rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-between;
}
.subD .subData .data-container {
  width: 47%;
  height: 220rpx;
  flex: none;
}
.subD .subData .data-container switch {
  margin-right: 20rpx;
}
.subD .subData .data-container .data-box {
  width: 100%;
  height: 120rpx;
  border: 1px solid #c8c8c8;
  border-radius: 5px;
  box-sizing: border-box;
  text-align: center;
  font: 50rpx/120rpx "微软雅黑", "Micosoft Yahei", "Micosoft YaHei", "sans-serif";
}
.subD .expand {
  color: white;
  width: 100%;
  height: 60rpx;
  font: 35rpx/60rpx "微软雅黑", "Micosoft Yahei", "Micosoft YaHei", "sans-serif";
  background-color: rgba(0, 161, 233, 0.6);
}

